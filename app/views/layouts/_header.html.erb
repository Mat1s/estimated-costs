<h1 class="text-center text-uppercase" style="color:#363636"> Cost estimate </h1>
	<% if user_signed_in? %>
		<h4 style="color:#363636" class="text-left text-capitalize"><%= current_user.name %></h4>
		<h4 style="color:#363636" class="text-left" ><%= link_to 'Sign out', destroy_user_session_path, method: :delete %></h4>
	<% else %>
		<%= link_to 'Log in', new_user_session_path %> |
  	<%= link_to 'Sign up', new_user_registration_path %> 
	<% end %>
